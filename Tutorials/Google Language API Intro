   "source": [
    "def print_top(obj, n=10):\n",
    "    if isinstance(obj,dict):\n",
    "        for k,v in obj.items():\n",
    "            if not isinstance(v,list):\n",
    "                print(k,\":\",v)\n",
    "            else:\n",
    "                print()\n",
    "                print(\"###\",k.title(),\"#####\")\n",
    "                print_top(v,n)\n",
    "    elif isinstance(obj, list):\n",
    "        for v in obj[:n]:\n",
    "            print()\n",
    "            print_top(v,n)\n",
    "\n",
    "# Print results. n= max number of results per item\n",
    "print_top(result, n=20)"
   ]
  }
